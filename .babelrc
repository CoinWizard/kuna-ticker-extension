{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "node": "current",
                    "uglify": true
                }
            }
        ],
        "react",
        "es2017",
        "es2016",
        "es2015",
        "stage-0"
    ],
    "retainLines": true,
    "plugins": [
        "dynamic-import-node",
        "transform-decorators-legacy",
        "transform-class-properties"
    ]
}